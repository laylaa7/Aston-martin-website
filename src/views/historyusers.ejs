<body style="color:white; background-color: black;">
    <div class="container mt-5">
        <div class="text-center">
            <h1 class="display-5 mb-5"><strong>Reservation History</strong></h1>
        </div>
        <div class="main row justify-content-center">
            <form action="" id="user-form" class="row justify-content-center mb-4" autocomplete="off">
                <div class="col-10 col-mid-8 mb-3">
                    <label for="firstName">FirstName</label>
                    <input class="form-control" id="firstName" type="text" placeholder="Enter FirstName">
                </div>
                <div class="col-10 col-mid-8 mb-3">
                    <label for="lastName">LastName</label>
                    <input class="form-control" id="lastName" type="text" placeholder="Enter LastName">
                </div>
                <div class="col-10 col-mid-8 mb-3">
                    <label for="email">Email Address</label>
                    <input class="form-control" id="email" type="email" placeholder="Enter Email">
                </div>
                <div class="col-10 col-mid-8 mb-3">
                    <label for="number">Telephone Number</label>
                    <input class="form-control" id="number" type="text" placeholder="Enter Number">
                </div>
                <div class="col-10 col-mid-8 mb-3">
                    <label for="date">Reservation</label>
                    <input class="form-control" id="date" type="text" placeholder="Enter Date">
                </div>
                <div class="col-10 col-mid-8 mb-3">
                    <label for="model">Car Model</label>
                    <input class="form-control" id="model" type="text" placeholder="Enter Number">
                </div>

                <div class="col-10 col-mid-8">
                    <input class="btn btn-success add-btn" type="submit" value="submit" id="submit">
                </div>
            </form>
            <div class="col-17 col-mid-10 mt-5">
                <table class="table table-striped table-dark">
