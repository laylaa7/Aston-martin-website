        <a class="a" href="/src/views/testdrive.html">Test Drive</a>
        <a class="a" href="#aboutus">About Us</a>
        <a class="a" href="#contactUs">Contact Us</a>
        <a class="a" href="/reservationHistory">Reservations</a>
        <a class="a" href="/testdriveHistory">testDrive History </a>
        <a class="a" href="/userHistory">Users Info</a>
        <a class="a" href="adminDash.html">Products Info</a>
    </div>
    <script>
        document.getElementById('Home').onclick = function () {
            location.reload();
        };
    </script>

    <!--Main Home-->


    <section class="home" id="home">
        <div class="vid-container">
            <video autoplay loop muted class="video" id="vid">
