    <title>Aston Martin</title>
</head>

<body>
    <!--Navigation Bar-->
    <header>
        <div class="open-menu">
            <div class="menu-btn" onclick="toggleNav()" id="menuBtn">
                <div class="bar1"></div>
                <div class="bar2"></div>
                <div class="bar3"></div>
            </div>
        </div>
        <div class="logo">
