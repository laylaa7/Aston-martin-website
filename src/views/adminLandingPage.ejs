    <!-- chat bot -->
    <div class="main-div">
        <div class="menuToggle">
            <i class='bx bx-help-circle'></i>
        </div>
        <div class="containeramgad">
            <div class="top-part">
                <div class="agent-details">
                    <h3>ChatBot</h3>
                </div>
            </div>
            <div class="chart-section">
                <div class="agent-chart">
                    <div class="left-part">
