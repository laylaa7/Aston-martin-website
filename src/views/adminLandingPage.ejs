                Your browser does not support the video tag.
            </video>
            <div class="text-box" id="text-box">
                <h1>ASTON MARTIN</h1>
            </div>
        </div>
        <a href="#models"><img class="arrow" src="/assets/img/arrows.png" alt="arrow" width="40"></a>
    </section>

    <section class="home2" id="home2">

    </section>

    <!--Models-->
    <div class="main-title" style="background-color: black;">
        <p>Popular Models</p>
    </div>
    <section class="models" id="models">
        <div class="container">
